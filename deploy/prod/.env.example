DB_PASSWORD=change-me

# Logto (API runtime)
LOGTO_ENDPOINT=https://auth.ternity.xyz
LOGTO_APP_ID=<from-logto-admin>

# Web build args
VITE_LOGTO_ENDPOINT=https://auth.ternity.xyz
VITE_LOGTO_APP_ID=<from-logto-admin>

# Sync service
TOGGL_API_TOKEN=<from-toggl>
TOGGL_WORKSPACE_ID=<from-toggl>
TOGGL_ORGANIZATION_ID=<from-toggl>
TIMETASTIC_API_TOKEN=<from-timetastic>
SYNC_FREQUENT_HOURS=4
SYNC_DAILY_HOUR_UTC=3

# Jira OAuth (API runtime)
JIRA_CLIENT_ID=<from-atlassian-developer-console>
JIRA_CLIENT_SECRET=<from-atlassian-developer-console>
JIRA_CALLBACK_URL=https://app.ternity.xyz/integrations/jira/callback

# Jira OAuth (web build args)
VITE_JIRA_CLIENT_ID=<same-as-JIRA_CLIENT_ID>
VITE_JIRA_CALLBACK_URL=https://app.ternity.xyz/integrations/jira/callback

# Notifications providers (API runtime)
RESEND_API_KEY=<from-resend>
RESEND_FROM_EMAIL=noreply@ternity.xyz
TWILIO_ACCOUNT_SID=<from-twilio>
TWILIO_AUTH_TOKEN=<from-twilio>
TWILIO_MESSAGING_SERVICE_SID=<from-twilio>
