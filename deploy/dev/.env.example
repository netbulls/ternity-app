DB_PASSWORD=change-me

# Logto (API runtime)
LOGTO_ENDPOINT=https://dev.auth.ternity.xyz
LOGTO_APP_ID=<from-logto-admin>

# Web build args
VITE_LOGTO_ENDPOINT=https://dev.auth.ternity.xyz
VITE_LOGTO_APP_ID=<from-logto-admin>

# Sync service
TOGGL_API_TOKEN=<from-toggl>
TOGGL_WORKSPACE_ID=<from-toggl>
TOGGL_ORGANIZATION_ID=<from-toggl>
TIMETASTIC_API_TOKEN=<from-timetastic>
SYNC_FREQUENT_HOURS=4
SYNC_DAILY_HOUR_UTC=3

# Jira OAuth (API runtime)
JIRA_CLIENT_ID=<from-atlassian-developer-console>
JIRA_CLIENT_SECRET=<from-atlassian-developer-console>
JIRA_CALLBACK_URL=https://dev.app.ternity.xyz/integrations/jira/callback

# Jira OAuth (web build args)
VITE_JIRA_CLIENT_ID=<same-as-JIRA_CLIENT_ID>
VITE_JIRA_CALLBACK_URL=https://dev.app.ternity.xyz/integrations/jira/callback
